{
  "name": "vite-electron-builder",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "buildEnvTypes": "node scripts/buildEnvTypes.js",
    "build": "node scripts/build.js",
    "buildWeb": "vite build -c ./web/vite.config.js",
    "watchWeb": "vite -c ./web/vite.config.js --port $PORT",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir --config.asar=false",
    "pretest": "cross-env MODE=test npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "pretypecheck": "npm run buildEnvTypes",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer"
  },
  "browserslist": [
    "Chrome 91"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run typecheck"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@iconify/json": "^1.1.387",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@vercel/node": "^1.12.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "lint-staged": "^11.1.2",
    "simple-git-hooks": "^2.5.1",
    "spectron": "^15.0.0",
    "typescript": "^4.3.5",
    "unplugin-auto-import": "^0.4.1",
    "unplugin-icons": "^0.7.5",
    "unplugin-vue-components": "^0.14.11",
    "vite": "^2.5.0",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-icons": "^0.6.5",
    "vite-plugin-pages": "^0.17.3",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.2.7",
    "vue-tsc": "^0.2.2",
    "windicss": "^3.1.7"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0-rc",
    "@headlessui/vue": "^1.4.0",
    "@netlify/functions": "^0.7.2",
    "@vueuse/core": "^6.1.0",
    "@vueuse/electron": "^5.3.0",
    "axios": "^0.21.1",
    "currency-codes": "^2.1.0",
    "currency-symbol-map": "^5.0.1",
    "dayjs": "^1.10.6",
    "electron-store": "^8.0.0",
    "electron-tray-window": "^1.2.5",
    "electron-traywindow-positioner": "^1.1.0",
    "electron-updater": "^4.4.5",
    "express": "^4.17.1",
    "monobank-api-client": "^1.0.2",
    "pinia": "^2.0.0-0",
    "qrcode": "^1.4.4",
    "swiper": "^6.8.1",
    "uuid": "^8.3.2",
    "vite-svg-loader": "^2.1.1",
    "vue": "3.2.2",
    "vue-global-api": "^0.4.1",
    "vue-router": "^4.0.11",
    "vue3-perfect-scrollbar": "^1.5.5"
  }
}
